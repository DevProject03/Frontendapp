 stage('SonarQube analysis') {
            steps {
                script{
                    def scannerHome = tool 'SonarScanner';
                    withSonarQubeEnv('sonarqube-9.7.1') {
                        sh "${tool("SonarScanner")}/bin/sonar-scanner \
                        -Dsonar.projectKey=frontendapp \
                        -Dsonar.projectName=frontendapp"
                    }
                }
            }
